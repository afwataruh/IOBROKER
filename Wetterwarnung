on({id: "fhem.0.OUT_Unwetterzentrale.WarnCount"/*OUT_Unwetterzentrale WarnCount*/, change: "ne"}, function (obj) {
  var value = obj.state.val;
  var oldValue = obj.oldState.val;
  if (getState('2').val) {
    sendTo("telegram", "send", {
        text: getState("fhem.0.OUT_Unwetterzentrale.Warn_0_IconURL").val
    });
  }
});


